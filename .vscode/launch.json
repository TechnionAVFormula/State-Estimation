    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: parse state message",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Code_Offline/SystemRunnerDebugger/messages_parser.py",
            "args": ["${workspaceFolder}/Code_RealTime/Messages/state.messages"],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "justMyCode": false
        },
        {
            "name": "Python: StateMain",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/Code_RealTime/StateMain.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/Code_RealTime"
        }
    ]
}